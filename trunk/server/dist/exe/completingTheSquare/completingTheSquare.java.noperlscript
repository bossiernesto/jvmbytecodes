package exe.completingTheSquare;

import exe.*;
import java.io.*;
import java.lang.*;
import java.util.*;

public class completingTheSquare {

    public static void main(String args[]) throws IOException {

	String[] params = new String[4];

	String filename = args[0] + ".xaal";
	params[0] = filename;

	if (args.length == 4)
	{
	    for(int i=1; i<4; i++)
		params[i] = args[i];

	    Quadratic.main(params);
	}
	else
	    Quadratic.main( new String[]{ filename } );
    }
}
